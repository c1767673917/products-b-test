# 前端环境配置

# API配置
VITE_API_BASE_URL=http://localhost:3000/api/v1
VITE_IMAGE_BASE_URL=http://your-image-server:9000

# 是否使用后端API (true/false)
# 设置为 false 时使用本地JSON数据，设置为 true 时使用后端API
VITE_USE_BACKEND_API=false

# 开发模式配置
VITE_DEV_MODE=true
VITE_API_TIMEOUT=10000

# 图片服务配置
VITE_ENABLE_IMAGE_OPTIMIZATION=true
VITE_DEFAULT_IMAGE_QUALITY=85
